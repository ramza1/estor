<%= content_for(:breadcrumb) do %>
    <li>
      <%= link_to "Dashboard", admin_root_url %>
      <span class="icon-angle-right"></span>
    </li>
    <li>
      <%= link_to "Shiping categories", admin_config_shipping_categories_path %>
      <span class="icon-angle-right"></span>
    </li>
<% end %>


<div class="row-fluid">
  <div class="span12">
    <div class="social-box">
      <div class="header">
        <h4>Editing Shipping Category <%= @shipping_category.name %></h4>
      </div>
      <div class="body">

        <%= simple_form_for(@shipping_category, :url => admin_config_shipping_category_path(@shipping_category), html: {class: "form-horizontal"}) do |f| %>
            <%= render 'form', f:  f %>
        <% end %>
      </div>
    </div>
  </div>
</div>

