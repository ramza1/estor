<table class="table table-striped table-bordered dataTable" style="margin-top: 10px;">
  <thead>
  <tr>
    <th>Name</th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  <% @prototypes.each do |prototype| %>
      <tr>
        <td><%= prototype.name %></td>
        <td>
          <div class="btn-group">
            <%= link_to  edit_admin_prototype_path(prototype), class: "btn" do %>
                <i class="icon-edit"></i> Edit
            <% end %>
            <%= link_to admin_prototype_path(prototype), method: :delete, class: "btn btn-danger", data: {confirm: "Are you sure"} do %>
                <i class="icon-trash"></i>    Delete
            <% end %>
          </div>
        </td>

      </tr>
  <% end %>
  </tbody>
</table>
