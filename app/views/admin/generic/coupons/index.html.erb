<%= content_for(:breadcrumb) do %>
    <li>
      <%= link_to "Dashboard", admin_root_url %>
      <span class="icon-angle-right"></span>
    </li>
<% end %>


<div class="row-fluid">
  <div class="span12">
    <div class="social-box">
      <div class="header">
        <h4>Coupons</h4>
        <div class="btn-group hidden-phone pull-right">
          <%= link_to "New Coupon", new_admin_generic_coupon_path, class: "btn btn-primary" %>
        </div>
      </div>
      <div class="body">
        <% if @coupons.empty? %>
            <p class="alert alert-info">
              No Coupons Available Yet
            </p>
        <% else %>
            <%= render "list" %>
        <% end %>
      </div>
    </div>
  </div>
</div>


