<table class="table table-striped table-bordered dataTable" style="margin-top: 10px;">
  <thead>
  <tr>
    <th>Identifing Name</th>
    <th>Display Name</th>
    <th>Active</th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  <% @properties.each do |property| %>
      <tr>
        <td><%= property.identifing_name %></td>
        <td><%= property.display_name %></td>
        <td><%= property.display_active %></td>
        <td>
          <div class="btn-group">
            <%= link_to  edit_admin_property_path(property), class: "btn btn-small" do %>
                <i class="icon-edit"></i> Edit
            <% end %>
            <%= link_to admin_property_path(property), method: :delete, class: "btn btn-danger btn-small", data: {confirm: "Are you sure"} do %>
                <i class="icon-trash"></i> Delete
            <% end %>
          </div>
        </td>

      </tr>
  <% end %>
  </tbody>
</table>
